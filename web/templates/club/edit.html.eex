<div class="form-header">
  <div class="header-text">
    Edit Club
  </div>
  <div class="form-header-nav">
    <%= link "Back", to: club_path(@conn, :index) %>
  </div>
</div>

<%= render "form.html", changeset: @changeset, action: club_path(@conn, :update, @club) %>

<%= link "Leave Club", to: club_club_path(@conn, :leave, @club), method: :delete, data: [confirm: "Are you sure you want to leave the club?"], class: "pull-right" %>
