<h2>Show message</h2>

<ul>

  <li>
    <strong>Room:</strong>
    <%= @message.room_id %>
  </li>

  <li>
    <strong>User:</strong>
    <%= @message.user_id %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @message.body %>
  </li>

</ul>

<%= link "Edit", to: message_path(@conn, :edit, @message) %>
<%= link "Back", to: message_path(@conn, :index) %>
